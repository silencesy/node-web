<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','full_name','$event',[]],['addressData']]]]]}}" value="{{addressData.full_name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="row b-b" bindtap="__e"><text class="tit">地址</text><text class="input">{{''+addressData.address_name+''}}</text><text class="yticon icon-shouhuodizhi"></text></view><view class="row b-b"><text class="tit">门牌号</text><input class="input" type="text" placeholder="楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','house_number','$event',[]],['addressData']]]]]}}" value="{{addressData.house_number}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.is_default}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><block wx:if="{{manageType=='add'}}"><view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">保存</button></view></block><block wx:else><view class="bottom-btns"><button data-event-opts="{{[['tap',[['deleteAddress',['$event']]]]]}}" class="delete-btn" bindtap="__e">删除</button><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="edit-btn" bindtap="__e">保存</button></view></block></view>